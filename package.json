{
  "name": "cyberpunk-beavers",
  "type": "module",
  "scripts": {
    "dev": "yarn nodemon js/server/main.mjs",
    "serve:dev": "yarn live-server --port=9001 --open=index.html?env=dev",
    "serve:local": "yarn live-server --port=9001 --open=index.html?env=local",
    "serve:prod": "yarn live-server --port=9001 --open=index.html?env=prod",
    "deploy-contract:local": "yarn build && node js/deploy-spawn.js --env local",
    "deploy-contract:prod": "yarn build && node js/deploy-spawn.js --env prod",
    "test": "node build.js && vitest",
    "build": "node build.js"
  },
  "dependencies": {
    "warp-contracts": "^1.4.45",
    "warp-contracts-plugin-signature": "^1.0.20",
    "warp-contracts-plugin-quickjs": "^1.1.5",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "live-server": "1.2.1",
    "esbuild": "^0.20.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "replace-in-file": "^7.1.0",
    "vitest": "^1.6.0",
    "warp-arbundles": "^1.0.4",
    "warp-contracts-plugin-deploy": "^1.0.13",
    "replace-in-files": "^3.0.0"
  }
}
